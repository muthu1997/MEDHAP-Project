{
  "expo": {
    "name": "MEDHAP",
    "slug": "MEDHAP",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#333"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.medhaphygenic.userPortalApplication",
      "infoPlist": {
        "CameraUsageDescription": "This app uses the camera to scan barcodes on warranty module.",
        "NSCameraUsageDescription": "This app uses the camera to make a photo"
      }
    },
    "android": {
      "package": "com.medhaphygenic.userPortalApplication",
      "versionCode": 1,
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true
    },
    "description": ""
  }
}
